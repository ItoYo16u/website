{
  "name": "static-site-template",
  "private": true,
  "description": "Gastby with TypeScript,MDX,emotion",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build-serve": "npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d public -b master -r https://github.com/ItoYo16u/blog",
    "extreme-deploy": "npm run format && npm run refresh  && npm run deploy",
    "extreme-develop": "npm run format && npm run refresh && npm run develop",
    "develop": "gatsby develop --host 0.0.0.0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,pcss}\"",
    "watch": "npm run format && onchange 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.pcss'  -- prettier --write {{changed}}",
    "clean": "gatsby clean",
    "open": "gatsby serve -p 8000 -o --host 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve -p 8000 --host 0.0.0.0",
    "test": "jest --color"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@welldone-software/why-did-you-render": "^4.0.4",
    "gatsby": "^2.31.1",
    "gatsby-background-image": "^1.4.0",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-catch-links": "^2.9.0",
    "gatsby-plugin-emotion": "^4.1.20",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-google-gtag": "^2.8.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-lodash": "^3.10.0",
    "gatsby-plugin-mdx": "^1.9.0",
    "gatsby-plugin-postcss": "^2.1.18",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.14.0",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-plugin-typescript": "^2.11.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-source-filesystem": "^2.1.45",
    "gatsby-transformer-sharp": "^2.12.0",
    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "postcss": "^7.0.26",
    "postcss-scss": "^2.0.0",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-twemoji": "^0.3.0",
    "remark-html-katex": "^3.0.0",
    "remark-math": "^3.0.1",
    "tailwind.macro": "^1.0.0-alpha.10",
    "tailwindcss": "^1.1.4",
    "ts-essentials": "^7.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-test-renderer": "^16.9.1",
    "@types/twemoji": "^12.1.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.26",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^16.12.0",
    "typescript-styled-plugin": "^0.15.0"
  }
}
